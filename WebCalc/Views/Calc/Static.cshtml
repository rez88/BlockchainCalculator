@model IEnumerable<ITUniver.Calc.DB.Models.HistoryItem>

@{
    ViewBag.Title = "Статистика";
}

<h2>Статистика</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Operation.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExecDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExecTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Operation.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Result)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExecDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExecTime)
        </td>
    </tr>
}

</table>
